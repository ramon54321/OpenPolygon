{
    "extends": ["react", "google"],
	"parser": "babel-eslint",
	"rules": {
		"no-tabs": ["off"],
		"no-undef": ["off"],
		"indent": ["off"],
		"object-curly-spacing": ["off"],
		"linebreak-style": ["off"],
		"quotes": ["warn", "double"],
		"semi": ["warn", "never"],
		"require-jsdoc": ["error", {
			"require": {
				"FunctionExpression": true
			}
		}]
	}
}
